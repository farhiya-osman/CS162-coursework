all: debug_demo debug_1 debug_2 debug_3

debug_demo: debug_demo.cpp
	g++ debug_demo.cpp -g -o debug_demo

debug_1: debug_1.cpp
	g++ debug_1.cpp -g -o debug_1

debug_2: debug_2.cpp
	g++ debug_2.cpp -g -o debug_2

debug_3: debug_3.cpp
	g++ debug_3.cpp -g -o debug_3

clean:
	rm -f debug_demo debug_1 debug_2 debug_3
